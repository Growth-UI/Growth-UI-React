{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.5%, not dead"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "minify",
      {
        "builtIns": false,
        "evaluate": false,
        "mangle": false
      }
    ]
  ],
  "comments": false,
  "sourceMaps": false,
  "plugins": [
    [
      "babel-plugin-tsconfig-paths",
      {
        "relative": true,
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "rootDir": ".",
        "tsconfig": "tsconfig.json"
      }
    ],
    "lodash",
    "styled-components",
    "@babel/plugin-transform-runtime"
  ]
}
